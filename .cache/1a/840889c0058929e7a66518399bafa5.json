  });\n\n  addEvent(contentEl, 'selectstart', (evt: Event) => {\n    evt.stopPropagation();\n    cancelAnimationFrame(animationID);\n\n    isSelected = true;\n  });\n\n  addEvent(window, 'mouseup blur', () => {\n    cancelAnimationFrame(animationID);\n\n    isSelected = false;\n  });\n\n  // patch for touch devices\n  addEvent(containerEl, 'scroll', (evt: Event) => {\n    evt.preventDefault();\n    containerEl.scrollTop = containerEl.scrollLeft = 0;\n  });\n}\n\nfunction calcMomentum(\n  scrollbar: I.Scrollbar,\n  evt: MouseEvent,\n) {\n  const { top, right, bottom, left } = scrollbar.bounding;\n  const { x, y } = getPosition(evt);\n\n  const res = {\n    x: 0,\n    y: 0,\n  };\n\n  const padding = 20;\n\n  if (x === 0 && y === 0) return res;\n\n  if (x > right - padding) {\n    res.x = (x - right + padding);\n  } else if (x < left + padding) {\n    res.x = (x - left - padding);\n  }\n\n  if (y > bottom - padding) {\n    res.y = (y - bottom + padding);\n  } else if (y < top + padding) {\n    res.y = (y - top - padding);\n  }\n\n  res.x *= 2;\n  res.y *= 2;\n\n  return res;\n}\n"},"lineCount":null}},"error":null,"hash":"d1ebaa66e140f0e636e139a4197ea5e5","cacheData":{"env":{}}}