ode","generated":{"line":33,"column":4},"source":"scripts/dark-mode.js","original":{"line":37,"column":4}},{"generated":{"line":33,"column":19},"source":"scripts/dark-mode.js","original":{"line":37,"column":19}},{"generated":{"line":34,"column":0},"source":"scripts/dark-mode.js","original":{"line":38,"column":3}},{"generated":{"line":35,"column":0},"source":"scripts/dark-mode.js","original":{"line":39,"column":1}},{"generated":{"line":35,"column":1},"source":"scripts/dark-mode.js","original":{"line":28,"column":0}}],"sources":{"scripts/dark-mode.js":"// check for saved 'darkMode' in localStorage\n\nlet darkMode = localStorage.getItem('darkMode');\n\nconst darkModeToggle = document.querySelector('#dark-mode-toggle');\n\nconst enableDarkMode = () => {\n  // 1. Add the class to the body\n  document.body.classList.add('darkmode');\n  // 2. Update darkMode in localStorage\n  localStorage.setItem('darkMode', 'enabled');\n}\n\nconst disableDarkMode = () => {\n  // 1. Remove the class from the body\n  document.body.classList.remove('darkmode');\n  // 2. Update darkMode in localStorage \n  localStorage.setItem('darkMode', null);\n}\n \n// If the user already visited and enabled darkMode\n// start things off with it on\nif (darkMode === 'enabled') {\n  enableDarkMode();\n}\n\n// When someone clicks the button\ndarkModeToggle.addEventListener('click', () => {\n  // get their darkMode setting\n  darkMode = localStorage.getItem('darkMode'); \n  \n  // if it not current enabled, enable it\n  if (darkMode !== 'enabled') {\n    enableDarkMode();\n  // if it has been enabled, turn it off  \n  } else {  \n    disableDarkMode(); \n  }\n});\n"},"lineCount":null}},"error":null,"hash":"b797cd53ec7c9cee01d7cb53b5129c5b","cacheData":{"env":{}}}